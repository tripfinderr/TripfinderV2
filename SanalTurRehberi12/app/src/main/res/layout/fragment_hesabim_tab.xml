<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HesabimTab"

    >

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout"

        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="200dp"
            android:layout_marginBottom="31dp"
            android:background="@drawable/maskkkk"
            app:layout_constraintBottom_toTopOf="@+id/HesapBilgileri"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0">

            <TextView
                android:id="@+id/textView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/poppinsbold"
                android:text="UserNickName"
                android:textColor="#FFFF"
                android:textSize="20sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.495"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/imageView3"
                app:layout_constraintVertical_bias="0.225" />

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="102dp"
                android:layout_height="68dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.196"
                app:srcCompat="@drawable/manuserbig" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <TextView
            android:id="@+id/HesapBilgileri"
            android:layout_width="0dp"
            android:layout_height="43dp"
            android:layout_marginBottom="10dp"
            android:drawableStart="@drawable/manuser"
            android:drawableLeft="@drawable/manuser"
            android:drawableEnd="@drawable/ic_baseline_arrow_forward_ios_24"
            android:drawableRight="@drawable/ic_baseline_arrow_forward_ios_24"
            android:fontFamily="@font/poppinsbold"
            android:gravity="center_horizontal"
            android:onClick="HhesapBilgileri"
            android:text="Hesap Bilgileri"
            android:textColor="#3F51B5"
            android:textSize="20sp"
            app:layout_constraintBottom_toTopOf="@+id/textView7"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent">

        </TextView>

        <TextView
            android:id="@+id/textView7"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp"
            android:drawableStart="@drawable/travelerwithasuitcase"
            android:drawableLeft="@drawable/travelerwithasuitcase"
            android:drawableEnd="@drawable/ic_baseline_arrow_forward_ios_24"
            android:drawableRight="@drawable/ic_baseline_arrow_forward_ios_24"
            android:fontFamily="@font/poppinsbold"
            android:gravity="center_horizontal"
            android:onClick="gittigimTurlar"
            android:text="Gittiğim Turlar"
            android:textColor="#3F51B5"
            android:textSize="20sp"
            app:layout_constraintBottom_toTopOf="@+id/textView8"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent">

        </TextView>

        <TextView
            android:id="@+id/textView8"

            android:drawableEnd="@drawable/ic_baseline_arrow_forward_ios_24"
            android:drawableRight="@drawable/ic_baseline_arrow_forward_ios_24"
            android:layout_width="0dp"
            android:layout_height="43dp"
            android:layout_marginTop="20dp"
            android:drawableStart="@drawable/notes"
            android:drawableLeft="@drawable/notes"
            android:fontFamily="@font/poppinsbold"
            android:gravity="center_horizontal"
            android:text="Notlarım"
            android:onClick="NotlarımPage"
            android:textColor="#3F51B5"
            android:textSize="20sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView7">

        </TextView>

        <TextView
            android:drawableEnd="@drawable/ic_baseline_arrow_forward_ios_24"
            android:drawableRight="@drawable/ic_baseline_arrow_forward_ios_24"
            android:id="@+id/textView9"
            android:layout_width="0dp"
            android:layout_height="43dp"
            android:layout_marginTop="10dp"
            android:drawableStart="@drawable/support"
            android:drawableLeft="@drawable/support"
            android:fontFamily="@font/poppinsbold"
            android:gravity="center_horizontal"
            android:text="Destek"
            android:textColor="#3F51B5"
            android:textSize="20sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView8">

        </TextView>

        <TextView
            android:drawableEnd="@drawable/ic_baseline_arrow_forward_ios_24"
            android:drawableRight="@drawable/ic_baseline_arrow_forward_ios_24"
            android:id="@+id/textView10"
            android:onClick="SSS"
            android:layout_width="0dp"
            android:layout_height="43dp"
            android:layout_marginTop="10dp"
            android:drawableStart="@drawable/help32"
            android:drawableLeft="@drawable/help32"
            android:fontFamily="@font/poppinsbold"
            android:gravity="center"
            android:text="SSS"
            android:textColor="#3F51B5"
            android:textSize="20sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView11"
            app:layout_constraintVertical_bias="0.035">

        </TextView>

        <TextView
            android:drawableEnd="@drawable/ic_baseline_arrow_forward_ios_24"
            android:drawableRight="@drawable/ic_baseline_arrow_forward_ios_24"
            android:id="@+id/textView11"
            android:onClick="HesapToFav"
            android:layout_width="0dp"
            android:layout_height="43dp"
            android:layout_marginTop="10dp"
            android:drawableStart="@drawable/favorite"
            android:drawableLeft="@drawable/favorite"
            android:fontFamily="@font/poppinsbold"
            android:gravity="center"
            android:text="Favorilerim"
            android:textColor="#3F51B5"
            android:textSize="20sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView12">

        </TextView>

        <TextView
            android:drawableEnd="@drawable/ic_baseline_arrow_forward_ios_24"
            android:drawableRight="@drawable/ic_baseline_arrow_forward_ios_24"
            android:id="@+id/textView12"
            android:onClick="HesapToHavaDurumu"
            android:layout_width="0dp"
            android:layout_height="43dp"
            android:layout_marginTop="10dp"
            android:fontFamily="@font/poppinsbold"
            android:gravity="center_horizontal"
            android:text="Hava Durumu"
            android:textColor="#3F51B5"
            android:textSize="20sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView13">

        </TextView>

        <TextView
            android:drawableEnd="@drawable/ic_baseline_arrow_forward_ios_24"
            android:drawableRight="@drawable/ic_baseline_arrow_forward_ios_24"
            android:id="@+id/textView13"
            android:onClick="SeyehatPlanlariHesabimTab"
            android:layout_width="0dp"
            android:layout_height="43dp"
            android:layout_marginTop="10dp"
            android:drawableStart="@drawable/direction"
            android:drawableLeft="@drawable/direction"
            android:fontFamily="@font/poppinsbold"
            android:gravity="center_horizontal"
            android:text="Seyehat Planları"
            android:textColor="#3F51B5"
            android:textSize="20sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView9">

        </TextView>

    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>